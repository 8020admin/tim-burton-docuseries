{
  "name": "tim-burton-docuseries-functions",
  "description": "Backend API for Tim Burton Docuseries Platform",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "echo 'Linting skipped'",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@mux/mux-node": "^7.0.0",
    "@sendgrid/mail": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "typescript": "^4.9.0"
  },
  "private": true
}
